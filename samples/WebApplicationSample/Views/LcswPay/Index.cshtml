@{
    ViewData["Title"] = "利楚商务扫呗支付";
}
<nav aria-label="breadcrumb">
    <ol class="breadcrumb">
        <li class="breadcrumb-item"><a asp-controller="Home" asp-action="Index">首页</a></li>
        <li class="breadcrumb-item active" aria-current="page">@ViewData["Title"]</li>
    </ol>
</nav>
<br />
<table class="table table-bordered table-hover">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">产品</th>
            <th scope="col">接口</th>
            <th scope="col">#</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <th scope="row">1</th>
            <td>注册终端</td>
            <td><a href="http://www.lcsw.cn/doc/api/payment.html#注册终端（获取accesstoken）" target="_blank">注册终端</a></td>
            <td><a asp-controller="LcswPay" asp-action="Sign">立即测试</a></td>
        </tr>
        <tr>
            <th scope="row">2</th>
            <td>刷卡（条码）支付</td>
            <td><a href="http://www.lcsw.cn/doc/api/payment.html#刷卡（条码）支付" target="_blank">刷卡（条码）支付</a></td>
            <td><a asp-controller="LcswPay" asp-action="BarcodePay">立即测试</a></td>
        </tr>
        <tr>
            <th scope="row">3</th>
            <td>扫码支付</td>
            <td><a href="http://www.lcsw.cn/doc/api/payment.html#扫码支付（预支付）" target="_blank">扫码支付</a></td>
            <td><a asp-controller="LcswPay" asp-action="PrepayPay">立即测试</a></td>
        </tr>
        <tr>
            <th scope="row">4</th>
            <td>支付查询</td>
            <td><a href="http://www.lcsw.cn/doc/api/payment.html#支付查询" target="_blank">支付查询</a></td>
            <td><a asp-controller="LcswPay" asp-action="Query">立即测试</a></td>
        </tr>
        <tr>
            <th scope="row">5</th>
            <td>退款申请</td>
            <td><a href="http://www.lcsw.cn/doc/api/payment.html#退款申请" target="_blank">退款申请</a></td>
            <td><a asp-controller="LcswPay" asp-action="Refund">立即测试</a></td>
        </tr>
        <tr>
            <th scope="row">5</th>
            <td>退款查询</td>
            <td><a href="http://www.lcsw.cn/doc/api/payment.html#退款订单查询" target="_blank">退款查询</a></td>
            <td><a asp-controller="LcswPay" asp-action="RefundQuery">立即测试</a></td>
        </tr>
</table>